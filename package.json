    "@xstate/react": "^4.1.1",
    "xstate": "5.17.1"
  },
  "release-it": {
    "git": {
      "commit": true,
      "tag": true,
      "push": true
    },
    "github": {
      "release": true,
      "releaseName": "Release ${version}",
      "autoGenerate": true,
      "preRelease": false,
      "draft": false
    },
    "npm": {
      "skipChecks": true,
      "publish": true
